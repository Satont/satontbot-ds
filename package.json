{
  "name": "marmokbot",
  "version": "4.0.0",
  "description": "MarmokBot rewritten in typecsript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clear": "rm -rf dest",
    "start": "node ./dest/index",
    "dev": "nodemon",
    "migrate": "npx knex migrate:latest",
    "migrate:make": "npx knex migrate:make"
  },
  "author": "Satont <satontworldwide@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.1",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "discord.js": "^12.1.1",
    "dotenv": "^8.2.0",
    "humanize-duration": "^3.22.0",
    "knex": "^0.20.13",
    "lodash": "^4.17.15",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.6",
    "sequelize-typescript": "^1.1.0",
    "source-map-support": "^0.5.16"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "ts-node src/index.ts",
    "ext": "ts"
  }
}
